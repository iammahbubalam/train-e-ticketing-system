<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="700.0" prefWidth="800.0"
            style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/16"
            fx:controller="com.mahbubalam.traineticketingsystem.ChangePasswordController">
    <children>
        <Label layoutX="325.0" layoutY="26.0" text="Change Password ">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <PasswordField fx:id="oldPasswordField" layoutX="388.0" layoutY="161.0" onKeyReleased="#onKeyReleased"
                       prefHeight="25.0" prefWidth="262.0"/>
        <Label layoutX="174.0" layoutY="160.0" text="Old Password :">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <PasswordField fx:id="confirmPasswordField" layoutX="388.0" layoutY="285.0" onKeyReleased="#onKeyReleased"
                       prefHeight="25.0" prefWidth="262.0"/>
        <Label layoutX="174.0" layoutY="284.0" text="Confirm New Password :">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <PasswordField fx:id="newPasswordField" layoutX="388.0" layoutY="223.0" onKeyReleased="#onKeyReleased"
                       prefHeight="25.0" prefWidth="262.0"/>
        <Label layoutX="174.0" layoutY="222.0" text="New Password :">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <JFXButton fx:id="saveButton" alignment="TOP_CENTER" buttonType="RAISED" contentDisplay="TOP" layoutX="323.0"
                   layoutY="469.0" onAction="#onClickChangePassword" prefHeight="35.0" prefWidth="150.0"
                   ripplerFill="#a60000" style="-fx-background-color: #A60000;" text="Change Password" textFill="WHITE">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <Text fx:id="warning" fill="#bf3737" layoutX="269.0" layoutY="375.0" strokeType="OUTSIDE" strokeWidth="0.0"
              textAlignment="CENTER" wrappingWidth="340.13671875">
            <font>
                <Font size="18.0"/>
            </font>
        </Text>
    </children>
</AnchorPane>